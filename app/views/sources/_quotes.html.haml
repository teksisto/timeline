- if section.quotes.present?
  %b Quotes
  .container-fluid
    - for quote in section.quotes
      .row
        - if quote.versions.count == 1
          .col-sm-12
            %blockquote
              = temp_org_render(quote.versions.first.text)
        - else
          - for version in quote.versions
            .col-sm-6
              %blockquote
                = temp_org_render(version.text)
